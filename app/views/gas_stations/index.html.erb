<p id="notice"><%= notice %></p>

<h1>Estaciones de Servicio</h1>




  <div class="row">
    <% @gas_stations.each do |gas_station| %>
    <div class="col-sm-6 col-md-4">
      <div class="thumbnail">
      <%= image_tag gas_station.image, class: 'img-responsive center-block', style:'max-height:300;' %>
      </div>

      <div class="caption">
        <h3>Nombre:<%= gas_station.name %></h3>
        <td><%= gas_station.address %></td>

        <td><%= gas_station.id_station %></td>
        <td><%= link_to 'Show', gas_station , class: "btn btn-info " %></td>
        <td><%= link_to 'Edit', edit_gas_station_path(gas_station), class: "btn btn-warning" %></td>
        <td><%= link_to 'Destroy', gas_station, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-danger" %></td>
      </div>
    </div>
    <% end %>
</div>
<br>

<%= link_to 'New Gas Station', new_gas_station_path %>
