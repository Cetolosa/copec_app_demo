<p id="notice"><%= notice %></p>

<h1>Estaciones de Servicio</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Image</th>
      <th>Name</th>
      <th>Id station</th>
      <th colspan="3"></th>
    </tr>
  </thead>




div class="row">

    <div class="col-sm-6 col-md-4">

      <div class="thumbnail">

        <img src="https://tuplop.cl/37-big_default/tazon-t-vidrio-logo.jpg" alt="Super Tazón" class="img-responsive center-block" style="max-height:300px;">

        <div class="caption">

          <h3>Super Tazón</h3>

          <p>$4.980</a></p>

          <p><a href="#" class="btn btn-warning center-block" role="button">Comprar</a></p>

        </div>

      </div>

    </div>





  <tbody>
    <% @gas_stations.each do |gas_station| %>
      <tr>
        <td><%= gas_station.address %></td>
        <td><%= image_tag gas_station.image %></td>
        <td><%= gas_station.name %></td>
        <td><%= gas_station.id_station %></td>
        <td><%= link_to 'Show', gas_station %></td>
        <td><%= link_to 'Edit', edit_gas_station_path(gas_station) %></td>
        <td><%= link_to 'Destroy', gas_station, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

